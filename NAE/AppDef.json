{
    "name": "Jupyter Notebook for QLM (CPU)",
    "description": "Jupyter notebook for QLM",
    "author": "Isoard",
    "licensed": false,
    "appdefversion": 2,
    "classifications": [
        "QLM"
    ],
    "machines": [
        "n*"
    ],
    "vault-types": [
        "FILE",
        "BLOCK",
        "BLOCK_ARRAY",
        "OBJECT"
    ],
    "scale_max": 1,
    "commands": {
        "Notebook": {
            "path": "/usr/local/bin/nimbix_notebook",
            "name": "Launch notebook",
            "interactive": true,
            "description": "Start Jupyter notebook with Python 3 and myQLM",
            "url": "https://%PUBLICADDR%:5902/tree?token=%RANDOM64%",
            "parameters": {
                "-r": {
                    "name": "Requirements file",
                    "description": "Path to requirements.txt file",
                    "type": "FILE",
                    "required": false,
                    "positional": false
                },
                "basearg": {
                    "name": "base",
                    "description": "base url",
                    "type": "CONST",
                    "value": "-u",
                    "positional": true,
                    "required": true
                },
                "base": {
                    "name": "base",
                    "description": "base url",
                    "type": "CONST",
                    "value": "%BASEURL%",
                    "positional": true,
                    "required": true
                }
            }
        }
    },
    "image": {
        "type": "image/png",
        "data": ""
    }
}
